{
	"name": "@rtrq/core",
	"version": "0.0.1",
	"description": "RTRQ Core client SDK",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"clean": "rimraf .turbo node_modules dist"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {},
	"devDependencies": {
		"@rtrq/eslint-config": "workspace:*",
		"@rtrq/typescript-config": "workspace:*",
		"@rtrq/utils": "workspace:*",
		"@types/node": "^20.11.24",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3",
		"vitest": "^1.3.1"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	}
}