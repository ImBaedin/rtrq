{
	"name": "@rtrq/self-hosted",
	"version": "0.0.1",
	"description": "Self-hosted RTRQ server application",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"start": "node dist/index.js",
		"start:dev": "tsx src/index.ts",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"typecheck": "tsc --noEmit",
		"clean": "rimraf .turbo node_modules dist"
	},
	"keywords": ["rtrq", "real-time", "websocket", "self-hosted"],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@rtrq/manager": "workspace:*",
		"dotenv": "^16.5.0"
	},
	"devDependencies": {
		"@rtrq/eslint-config": "workspace:*",
		"@rtrq/typescript-config": "workspace:*",
		"@types/node": "^20.11.24",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"tsup": "^8.0.2",
		"tsx": "^4.7.1",
		"typescript": "^5.3.3"
	}
}